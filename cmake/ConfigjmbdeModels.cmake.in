# Determine the directory where the CMake file is located
get_filename_component(MODELS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# Add the directory to the CMake module path
list(APPEND CMAKE_MODULE_PATH ${MODELS_CMAKE_DIR})

# Find the Qt dependency
find_dependency(Qt @MIN_Qt_VERSION@)

# Remove the last added directory from the CMAKE_MODULE_PATH
list(REMOVE_AT CMAKE_MODULE_PATH -1)

# Check if the target jmuelbert::jmbdemodels exists
if (NOT TARGET jmuelbert::jmbdemodels)
    include("${MODELS_CMAKE_DIR}/jmbdeModelsTargets.cmake")
endif()

# Set the libraries for the models
set(Models_LIBRARIES jmuelbert::jmbdemodels)
